{
    "name": "drnkwati/repack-translation",
    "description": "Standalone port of Illuminate Translation package with extras ported for PHP 5.3.",
    "keywords": ["Translation", "Translator", "JSON Translator"],
    "license": "MIT",
    "homepage": "https://github.com/drnkwati/repack-translation",
    "support": {
        "source": "https://github.com/drnkwati"
    },
    "authors": [
        {
            "name": "Nkwati Divine",
            "email": "drnkwati@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.9"
    },
    "require-dev": {
        "drnkwati/repack-dumper": ">=1.0"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "Repack\\": "src"
        }
    },
    "autoload-dev": {
        "files": [
        ],
        "psr-4": {
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
